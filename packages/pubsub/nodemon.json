{
  "restartable": "rs",
  "ignore": [
    ".git",
    "dist"
  ],
  "verbose": true,
  "execMap": {
    "js": "babel-node",
    "ts": "babel-node"
  },
  "runOnChangeOnly": false,
  "watch": [
    "src/**/*.js",
    "src/**/*.ts",
    "src/**/*.graphql",
    "src/**/*.gql"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json,ts,graphql"
}
